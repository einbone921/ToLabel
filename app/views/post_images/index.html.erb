<div class="container">
  <h2>Logo</h2>
  <div>
    <%= form_with url:post_images_path, method: :get, local: true do |f| %>
      <%= f.text_field :word %>
      <%= f.submit "検索" %>
    <% end %>
  </div>
  <div>
    <% @post_images.each do |post| %>
      <div>
        <%= link_to post_image_path(post) do %>
          <%= attachment_image_tag post, :post_image, size:"200x200" %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>