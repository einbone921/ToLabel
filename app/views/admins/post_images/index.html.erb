<div class="container">
  <h1 class="logo">Logo</h1>
  <div class="row">
    <% @post_images.each do |post| %>
      <div class="image-item mt-4 shadow-lg">
        <%= link_to admins_post_image_path(post) do %>
          <%= attachment_image_tag post, :post_image, size:"200x200" %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>

